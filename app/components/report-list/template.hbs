<table class='table table-striped table-hover'>
  <thead>
    <tr>
      <th>
        UCID
      </th>
      <th>
        Name
      </th>
      <th>
        Major
      </th>
    </tr>
  </thead>
  <tbody>
    {{#each students as |student|}}
    <tr>
      <td>
        {{ student.ucid }}
      </td>
      <td>
        {{ student.firstName }} {{ student.lastName }}
      </td>
      <td>
        {{ student.major }}
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
